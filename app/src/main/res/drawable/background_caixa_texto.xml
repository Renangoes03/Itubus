<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape android:shape="rectangle">
            <solid android:color="#F5F5F5"/> <!-- Cor de fundo -->
            <stroke android:width="1dp" android:color="#2196F3"/> <!-- Cor da borda -->
            <corners android:radius="8dp"/> <!-- Bordas arredondadas -->
            <padding android:left="10dp" android:right="10dp" android:top="5dp" android:bottom="5dp"/> <!-- Padding -->
        </shape>
    </item>
</selector>
